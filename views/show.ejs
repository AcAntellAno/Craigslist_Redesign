<% include partials/header %>
    <div class="container">
        <div class="row">
            <div class="col-md-3">
                <p class="lead">Doug's List</p>
                <div class="list-group">
                    <li class="list-group-item active">Technology</li>
                    <li class="list-group-item">Gaming</li>
                    <li class="list-group-item">For Sale</li>
                </div>
            </div>
            <div class="col-md-9">
                <div class="thumbnail">
                    <img class="img-fluid" src="<%=items.image%>" alt="Item">
                    <div class="caption-full">
                        <h4 class="pull-right">
                            $
                            <%=items.price%>
                        </h4>
                        <h3>
                            <a href="#">
                                <%= items.name %>
                            </a>
                        </h3>
                        <h4>
                            <p>
                                <%=items.description%>
                            </p>
                        </h4>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <% include partials/footer %>